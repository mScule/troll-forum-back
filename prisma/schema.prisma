// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  password String
  events   Event[]
}

model Event {
  id       Int      @id @default(autoincrement())
  event    String
  date     DateTime
  place    String
  author   User     @relation(fields: [authorId], references: [id])
  authorId Int
}
